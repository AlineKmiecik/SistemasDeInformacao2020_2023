algoritmo "semnome"
var
   mult,divi,soma, sub, n1, n2:real

inicio
      escreval("Digite dois números inteiros:")
      leia(n1)
      escreval()
      leia(n2)
      limpatela
      Escreval("soma:", n1+n2)
      escreval("subtração:", n1-n2)
      escreval("multiplicação:", n1*n2)
      escreval("divisão:", n1/n2:1:2, " com resto igual a ", n1%n2)
      

fimalgoritmo
