Algoritmo "CRESCENTE"

Var

   n1,n2:real

Inicio

      escreva("1º VALOR:")
      leia(n1)
      escreva("2º VALOR:")
      leia(n2)
      se (n1>=n2) entao
         escreva(n2, ",", n1)
      senao
           se (n2>=n1) entao
              escreva(n1,",",n2)
           fimse
      fimse


Fimalgoritmo